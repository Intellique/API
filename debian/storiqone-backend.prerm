#! /bin/sh
set -e

# We need to test that /var/www/stone/cache/movies has correct permissions and user
if [ "$1" = "remove" -o "$1" = "purge" ] && [ -e /usr/share/apache2/apache2-maintscript-helper ]; then
	. /usr/share/apache2/apache2-maintscript-helper

	apache2_invoke disconf storiqone-backend.conf
fi
